{
  "description": "Schema for importing products via CSV file",
  "content": {
    "multipart/form-data": {
      "schema": {
        "type": "object",
        "required": ["file"],
        "properties": {
          "file": {
            "type": "string",
            "format": "binary",
            "description": "CSV file containing products to import"
          }
        }
      }
    }
  },
  "required": true
}
